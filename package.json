{
  "name": "mobx-autoform",
  "version": "0.8.0",
  "description": "Ridiculously simple form state management with mobx",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel ./src --out-dir ./lib --delete-dir-on-start",
    "test": "jest src",
    "test:watch": "jest --watch --",
    "test:ci": "jest --coverage --json --outputFile=test-results.json",
    "danger": "duti",
    "lint": "eslint src/",
    "lint:ci": "npm run lint -- -o lint-results.json -f json",
    "lint-fix": "eslint src/ --fix",
    "fmt": "prettier --write src/**/*.js",
    "duti:fix": "npm run fmt && npm run lint-fix && npm run fmt",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartprocure/mobx-autoform.git"
  },
  "keywords": [
    "mobx",
    "form",
    "mobx-form",
    "autoform",
    "mobx-autoform",
    "mobx-react-form"
  ],
  "author": {
    "name": "Samuel Greene",
    "email": "sgreene@smartprocure.us"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smartprocure/mobx-autoform/issues"
  },
  "homepage": "https://github.com/smartprocure/mobx-autoform#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "babel-eslint": "^10.0.3",
    "danger": "^9.2.10",
    "duti": "^0.15.2",
    "eslint": "^7.32.0",
    "eslint-config-smartprocure": "^2.0.1",
    "futil": "^1.69.0",
    "jest": "^27.0.6",
    "lodash": "^4.17.21",
    "mobx": "^4.15.4",
    "prettier": "^2.0.5"
  },
  "peerDependencies": {
    "futil": "^1.69.0",
    "lodash": "^4.17.21",
    "mobx": ">=4"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "dependencies": {},
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "sourceMaps": "inline",
    "retainLines": true
  },
  "eslintConfig": {
    "env": {
      "jest": true
    },
    "extends": [
      "smartprocure"
    ],
    "parser": "babel-eslint"
  }
}
