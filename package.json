{
  "name": "mobx-autoform",
  "version": "0.1.2",
  "description": "Ridiculously simple form state management with mobx",
  "main": "lib/mobx-autoform.js",
  "scripts": {
    "build": "webpack",
    "test": "jest src",
    "test:watch": "jest --watch src",
    "danger": "duti",
    "lint": "eslint src/",
    "lint:ci": "npm run lint -- -o lint-results.json -f json",
    "lint-fix": "eslint src/ --fix",
    "fmt": "prettier --write src/**/*.js",
    "duti:fix": "npm run fmt && npm run lint-fix && npm run fmt",
    "prepublish": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartprocure/mobx-autoform.git"
  },
  "keywords": [
    "mobx",
    "form",
    "mobx-form",
    "autoform",
    "mobx-autoform",
    "mobx-react-form"
  ],
  "author": {
    "name": "Samuel Greene",
    "email": "sgreene@smartprocure.us"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smartprocure/mobx-autoform/issues"
  },
  "homepage": "https://github.com/smartprocure/mobx-autoform#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-loader": "^8.0.6",
    "duti": "^0.15.2",
    "eslint": "^6.8.0",
    "eslint-config-smartprocure": "^1.2.1",
    "futil": "^1.65.0",
    "jest": "^25.1.0",
    "lodash": "^4.17.15",
    "mobx": "^5.15.4",
    "prettier": "^1.19.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "peerDependencies": {
    "futil": "^1.65.0",
    "lodash": "^4.17.15",
    "mobx": ">=4"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5"
  },
  "dependencies": {},
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "env": {
      "jest": true
    },
    "extends": [
      "smartprocure"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": "2019"
    }
  }
}
